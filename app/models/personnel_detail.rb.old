class PersonnelDetail < ActiveRecord::Base
  belongs_to :member, :foreign_key => "id"
  belongs_to :education, :foreign_key => "education_id"
  belongs_to :ministry, :foreign_key => "ministry_id"
  belongs_to :location
  belongs_to :employment_status, :foreign_key => "employment_status_id"
  belongs_to :status

  def to_label
    "#{member.last_name_first}"
  end

 
end
