<h1>Listing contacts</h1>

<table>
  <tr>
    <th>Person</th>
    <th>Email 1</th>
    <th>Email 2</th>
    <th>Phone 1</th>
    <th>Phone 2</th>
    <th>Blog</th>
    <th>Website</th>
    <th>Photosite</th>
    <th>Facebook</th>
    <th>Contact name</th>
    <th>Contact type</th>
    <th>Address</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @contacts.each do |contact| %>
  <tr>
    <td><%= contact.member_id %>
    <td><%= contact.email_1 %></td>
    <td><%= contact.email_2 %></td>
    <td><%= contact.phone_1 %></td>
    <td><%= contact.phone_2 %></td>
    <td><%= contact.blog %></td>
    <td><%= contact.other_website%></td>
    <td><%= contact.photos %></td>
    <td><%= contact.facebook %></td>
    <td><%= contact.contact_name %></td>
    <td><%= contact.contact_type_id %></td>
    <td><%= contact.address %></td>
    <td><%= link_to 'Show', contact %></td>
    <td><%= link_to 'Edit', edit_contact_path(contact) %></td>
    <td><%= link_to 'Destroy', contact, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Contact', new_contact_path %>
